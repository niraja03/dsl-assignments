# ---------- Function Definitions ----------

def linear_search(roll_numbers, key):
    for i in range(len(roll_numbers)):
        if roll_numbers[i] == key:
            return i
    return -1


def sentinel_search(roll_numbers, key):
    last = roll_numbers[-1]             # store last element
    roll_numbers[-1] = key              # place key at the end

    i = 0
    while roll_numbers[i] != key:       # loop until found
        i += 1

    roll_numbers[-1] = last             # restore last element

    if i < len(roll_numbers) - 1 or roll_numbers[-1] == key:
        return i
    else:
        return -1


# ---------- Main Program ----------

n = int(input("Enter total number of students: "))
roll_numbers = []

for i in range(n):
    roll = int(input(f"Enter roll number of student {i+1}: "))
    roll_numbers.append(roll)

key = int(input("\nEnter roll number to search: "))

# Linear Search
pos1 = linear_search(roll_numbers, key)
if pos1 != -1:
    print(f"Linear Search: Roll number found at position {pos1+1}")
else:
    print("Linear Search: Roll number not found")

# Sentinel Search
pos2 = sentinel_search(roll_numbers.copy(), key)
if pos2 != -1:
    print(f"Sentinel Search: Roll number found at position {pos2+1}")
else:
    print("Sentinel Search: Roll number not found")
